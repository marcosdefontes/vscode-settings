{
    "git.autofetch": true,
    "git.enableSmartCommit": true,
    "window.zoomLevel": 0,
    "vsicons.dontShowNewVersionMessage": true,
    "editor.fontFamily": "'Fira Code iScript','Fira Code','Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontLigatures": true,
